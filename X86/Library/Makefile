CURDIR=$(shell pwd)
PACKAGE_VERSION :=
PACKAGE_NAME := U8glib_Arduino
PACKAGE_DIR := U8glib_Arduino
PACKAGE_CONF :=
first_rule: all

all: build install

build:

config_rule:

install_rule install:
		$(warning ******************************* Install library u8glib_arduino **************************************)
		tar xvf $(CURDIR)/$(PACKAGE_DIR)/$(PACKAGE_NAME).tar.gz -C $(CURDIR)/../arduino/arduino-1.6.11/libraries/; \
		
clean_rule distclean clean:

uninstall_rule uninstall:

